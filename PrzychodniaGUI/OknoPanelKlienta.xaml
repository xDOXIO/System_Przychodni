<Window x:Class="PrzychodniaGUI.OknoPanelKlienta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrzychodniaGUI"
        mc:Ignorable="d"
        Title="OknoPanelKlienta" Height="450" Width="250">
    <Grid>
        <Button x:Name="btLekarze" Content="Wyświetl lekarzy" HorizontalAlignment="Left" Height="60" Margin="44,32,0,0" VerticalAlignment="Top" Width="160" FontSize="18" Click="btLekarze_Click"/>
        <Button x:Name="btUslugi" Content="Wyświetl usługi" HorizontalAlignment="Left" Height="60" Margin="44,121,0,0" VerticalAlignment="Top" Width="160" FontSize="18" Click="btUslugi_Click"/>
        <Button x:Name="btUmow" Content="Umów wizytę" HorizontalAlignment="Left" Height="60" Margin="44,210,0,0" VerticalAlignment="Top" Width="160" FontSize="18" Click="btUmow_Click"/>
        <Button x:Name="btWyjdz" Content="Wyloguj się" HorizontalAlignment="Left" Height="60" Margin="44,300,0,0" VerticalAlignment="Top" Width="160" FontSize="18" RenderTransformOrigin="0.421,1.932" Click="btWyjdz_Click"/>

    </Grid>
</Window>
